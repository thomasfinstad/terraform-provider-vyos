                  </leafNode>
                </children>
              </tagNode>
              <!-- include end -->
              <node name="format">
                <properties>
                  <help>Logging format</help>
                </properties>
                <children>
                  <leafNode name="octet-counted">
                    <properties>
                      <help>Allows for the transmission of all characters inside a syslog message</help>
                      <valueless/>
                    </properties>
                  </leafNode>
                </children>
