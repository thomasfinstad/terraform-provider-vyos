---
page_title: "{{.Name}} {{.Type}} - {{.ProviderName}}"
{{ $n := index (split .Name "_") 1 }}
{{ if eq $n "high" -}}
    subcategory: "High Availability"
{{ else -}}
    subcategory: "{{ title $n }}"
{{ end }}
description: |- {{/* Remove html formatting */}}
{{range $_, $e := split (.Description | plainmarkdown | trimspace) "\n" -}}
    {{- if ge (len $e) 1 -}}
        {{- $c := slice ($e | trimspace) 0 1 -}}
        {{- if and (ne $c "<") (ne $c "&") -}}
            {{- $e | prefixlines "  " }}{{"\n"}}
        {{- end -}}
    {{- end -}}
{{- end -}}
---

